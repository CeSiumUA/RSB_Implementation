<Window x:Class="RSB_GUI.Windows.GistoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSB_GUI.Windows" xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="GistoWindow" Height="450" Width="800">
    <Grid>
        <oxy:PlotView x:Name="Plot" Model="{Binding HistogramPlotModel}"/>
    </Grid>
</Window>
